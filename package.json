{
	"name": "vite-plugin-components",
	"version": "1.0.0",
	"description": "Components auto importing for Vue",
	"packageManager": "pnpm@7.17.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"scripts": {
		"example:dev": "pnpm -C ./examples/vue3 dev",
		"dev": "tsup --watch src",
		"test": "vitest",
		"prepack": "echo prepack",
		"build": "tsup",
		"prepublishOnly": "pnpm run build"
	},
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=14"
	},
	"author": "ljf1834",
	"license": "MIT",
	"dependencies": {
		"chokidar": "^3.5.3",
		"fast-glob": "^3.2.12",
		"magic-string": "^0.27.0",
		"minimatch": "^5.1.1"
	},
	"devDependencies": {
		"@types/node": "^18.11.9",
		"esno": "^0.16.3",
		"rollup": "^3.4.0",
		"tsup": "^6.5.0",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vitest": "^0.25.5",
		"vue": "3.2.37"
	}
}
